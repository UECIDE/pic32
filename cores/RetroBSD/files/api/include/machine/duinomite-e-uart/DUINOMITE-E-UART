# Console on UART5

core pic32mx7
mapping generic
linker bootloader-maximite

device kernel cpu_khz=80000 bus_khz=40000 led=B15

device console port=UART5
device uart5 baud=115200 power=G13

device rdisk led=C1
device sd0 port=3 cs=G12 power=G13

device gpio
